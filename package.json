    "migration:generate": "npm run typeorm migration:generate -- -d src/config/database.ts",
    "migration:run-specific": "ts-node scripts/run-specific-migration.ts",
    "migration:create": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:create",
    "seed:saas": "ts-node src/seeder/SaasOwnerSeeder.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
